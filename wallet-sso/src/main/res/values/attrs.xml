<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CheckBox_style">
        <attr name="size" format="dimension" />
        <attr name="color_border" format="color" />
        <attr name="color_background" format="color" />
    </declare-styleable>

    <attr name="item_title" format="string" />
    <attr name="item_content" format="string" />
    <declare-styleable name="ApplySubmitItemView">
        <attr name="item_title" />
        <attr name="item_content" />
    </declare-styleable>

    <declare-styleable name="ApplySubmitEditItemView">
        <attr name="item_title" />
        <attr name="item_content" />
        <attr name="android:inputType" />
        <attr name="android:digits" />
        <attr name="android:maxLength" />
        <attr name="item_hint" format="string" />
    </declare-styleable>

    <declare-styleable name="IDCardLayout">
        <attr name="icl_id_card_default" format="reference" />
        <attr name="icl_photograph_desc" format="reference|string" />
    </declare-styleable>

</resources>