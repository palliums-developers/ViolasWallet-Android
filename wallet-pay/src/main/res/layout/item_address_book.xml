<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    tools:context=".ui.addressBook.AddressBookActivity">

    <TextView
        android:id="@+id/tvCoinType"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_16"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_13"
        android:background="@drawable/bg_style_1_solid_corners_4_normal"
        android:gravity="center_vertical"
        android:paddingHorizontal="@dimen/dp_6"
        android:textColor="?android:attr/textColorTertiary"
        android:textSize="@dimen/fs_10"
        app:layout_constraintEnd_toEndOf="@+id/tvAddress"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="BTC" />

    <TextView
        android:id="@+id/tvTitle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_13"
        android:layout_marginEnd="@dimen/dp_8"
        android:ellipsize="end"
        android:lines="1"
        android:textColor="?android:attr/textColorSecondary"
        android:textSize="@dimen/fs_12"
        app:layout_constraintBottom_toBottomOf="@id/tvCoinType"
        app:layout_constraintEnd_toStartOf="@id/tvCoinType"
        app:layout_constraintStart_toStartOf="@id/tvAddress"
        tools:text="你好啊你好啊你好啊你好啊你好啊你好啊你好啊你好啊" />

    <TextView
        android:id="@+id/tvAddress"
        android:layout_width="@dimen/dp_0"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginBottom="@dimen/dp_10"
        android:background="@drawable/sel_bg_style_1_solid_corners_16"
        android:gravity="center_vertical"
        android:paddingHorizontal="@dimen/dp_13"
        android:paddingVertical="@dimen/dp_18"
        android:textSize="@dimen/fs_14"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvCoinType"
        tools:text="sdsdfsdfsdfsdfsdsdfsdfsdfsdfsdsdfsdfsdfsdfsdsdfsdfsdfsdfsdsdfsdfsdfsdf" />


</androidx.constraintlayout.widget.ConstraintLayout>